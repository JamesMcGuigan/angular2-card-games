{
  "name": "angular2-card-games",
  "version": "0.1.0",
  "homepage": "http://cards.jamesmcguigan.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesMcGuigan/angular2-card-games"
  },
  "scripts": {
    "ng": "ng",
    "clean": "rm -rvf ./dist",
    "start": "ng serve --open",
    "hmr": "ng serve --hmr -c hmr --open",
    "build": "ng build --prod -c production --aot --output-path ./dist",
    "watch": "ng build --prod -c production --aot --output-path ./dist --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "http-server": "http-server ./dist/angular2-card-games/",
    "deploy": "npm run deploy:aws",
    "deploy:aws": "npm run build && aws s3 sync ./dist/ s3://cards.jamesmcguigan.com --acl public-read",
    "now-build": "echo npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/pro-light-svg-icons": "^5.8.2",
    "@fortawesome/pro-regular-svg-icons": "^5.8.2",
    "@fortawesome/pro-solid-svg-icons": "^5.8.2",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/router-store": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "cards": "github:kbjr/node-cards#poker-hands",
    "cardsJS": "^1.1.1",
    "core-js": "^2.5.4",
    "deck-of-cards": "^0.1.8",
    "http-server": "^0.11.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@ngrx/schematics": "^7.4.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.125",
    "@types/node": "^8.10.48",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "now": "^15.3.0",
    "protractor": "~5.4.0",
    "tar": ">=4.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
